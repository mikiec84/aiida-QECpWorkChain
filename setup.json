{
    "name": "aiida-QECpWorkChain",
    "author": "Riccardo Bertossa",
    "author_email": "",
    "description": "Car-Parrinello Work Chain",
    "url": "https://github.com/rikigigi/aiida-QECpWorkChain",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.workflows": [
            "QECpWorkChain = aiida_QECpWorkChain.workflow:CpWorkChain"
        ],
        "aiida.calculations": [
            "DeepMdTrain = aiida_QECpWorkChain.workflow:DeepMdTrainCalculation"
        ],
        "aiida.parsers": [
            "DeepMdTrainParser = aiida_QECpWorkChain.workflow:DeepMdTrainParser"
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=1.0.0,<2.0.0",
        "six",
        "voluptuous"
    ],
    "extras_require": {
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=3.6,<5.0.0",
            "pytest-cov"
        ],
        "pre-commit": [
            "astroid==1.6.6; python_version<'3.0'",
            "astroid==2.2.5; python_version>='3.0'",
            "pre-commit==1.20.0",
            "prospector==1.1.7",
            "pylint-django<0.9; python_version<'3.0'",
            "pylint==1.9.4; python_version<'3.0'",
            "pylint==2.3.1; python_version>='3.0'"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme"
        ]
    }
}
